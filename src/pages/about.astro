<style>
  .text-page {
    max-width: 720px;
    display: grid;
    gap: calc(var(--u) * 2);
    font-size: 20px;
    text-align: center;
    margin: 0 auto;
  }

  .about-grid {
    display: grid;
    gap: calc(var(--u) * 4);
    align-items: center;
    justify-items: center;
    grid-template-columns: minmax(0, 1fr);
    width: 100%;
  }

  .about-content {
    display: grid;
    gap: calc(var(--u) * 3);
    justify-items: center;
    width: 100%;
  }

  .about-copy {
    margin: 0;
  }

  .about-links {
    display: grid;
    gap: calc(var(--u) * 2);
    justify-items: center;
    text-align: center;
    color: #f45b69;
    font-weight: 700;
  }

  .about-links a {
    color: inherit;
  }

  .about-links a:visited {
    color: inherit;
  }

  .about-cv {
    text-transform: lowercase;
    justify-self: center;
  }

  .about-cv:hover {
    text-decoration: underline;
    text-decoration-thickness: 0.12em;
    text-underline-offset: 0.18em;
  }

  .about-cv:focus-visible {
    text-decoration: underline;
    text-decoration-thickness: 0.12em;
    text-underline-offset: 0.18em;
  }

  .about-contact {
    margin: 0;
  }

  .about-email {
    text-decoration: underline;
    text-decoration-thickness: 0.12em;
    text-underline-offset: 0.18em;
  }

  .about-figure {
    width: auto;
  }

  .about-figure svg {
    display: block;
    width: auto;
    height: 200px;
  }

  @media (min-width: 769px) {
    .text-page {
      max-width: 980px;
      text-align: left;
    }

    .about-grid {
      grid-template-columns: auto auto;
      align-items: end;
      justify-content: center;
      column-gap: calc(var(--u) * 6);
      row-gap: calc(var(--u) * 3);
      justify-items: start;
    }

    .about-content {
      justify-items: start;
      max-width: 465px;
    }

    .about-links {
      justify-items: start;
      text-align: left;
    }

    .about-cv {
      justify-self: start;
    }

    .about-figure svg {
      height: 450px;
    }
  }
</style>
